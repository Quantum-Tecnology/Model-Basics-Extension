{
  "name": "quantumtecnology/model-basics-extension",
  "license": "MIT",
  "homepage": "http://www.quantumtecnology.esy.es/",
  "description": "Model Basics Extension",
  "require": {
    "php": "^8.0",
    "laravel/framework": "^10.0 | ^11.0| ^12.0",
    "quantumtecnology/setschema-trait-postgresql": "*"
  },
  "autoload": {
    "psr-4": {
      "QuantumTecnology\\ModelBasicsExtension\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Luis Gustavo Santarosa",
      "email": "gustavo-computacao@hotmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "gustavo-computacao@hotmail.com"
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "QuantumTecnology\\ModelBasicsExtension\\Provider\\ModelBasicsExtensionProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
